<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation.xml"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.test.machine.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_lockerFragment"
            app:destination="@id/lockerFragment" />
    </fragment>
    <fragment
        android:id="@+id/lockerFragment"
        android:name="com.test.machine.fragment.splash.locker.LockerFragment"
        android:label="LockerFragment"
        tools:layout="@layout/fragment_locker" />
</navigation>